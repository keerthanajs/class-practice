// print addition tables
#include <stdio.h>

int main()
{
    int num, i, n;
    printf("Enter a number: ");
    scanf("%d", &num);
    printf("Enter number upto which the series should end: ");
    scanf("%d", &n);
    
    printf("ADDITION TABLES\n");
    
    for(i = 1; i <= n; i++){
        printf("%d + %d = %d\n", num, i, num+i);
    }

    return 0;
}
