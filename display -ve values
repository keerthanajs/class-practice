//Write a C Program to get both +ve and -ve Numbers and display only -ve Numbers
#include <stdio.h>

int main()
{
    int a[10];
    
    
    for(int i = 0; i < 10; i++){
        printf("Enter array element: ");
        scanf("%d", &a[i]);    
    }
    
    printf("Negative Elements are: ");
    for(int i = 0; i < 10; i++){
        if(a[i] < 0){
            printf("%d ", a[i]);    
        }
        
    }
    
    return 0;
}
