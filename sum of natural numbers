// sum of natural numbers
#include <stdio.h>

int main()
{
    int n, sum, i = 1;
    
    printf("Enter a number: ");
    scanf("%d", &n);
    
    while(i <= n){
        sum = sum + i;
        //printf("Sum during loop is %d\n", sum);
        i++;
    }
    printf("Sum is %d", sum);

    return 0;
}
