#include<stdio.h>

struct complex_numbers{
    float real, img;
};

int main(){
    struct complex_numbers a, b, c; 
    
    printf("Enter 2 complex numbers :");
    scanf("%f%f", &a.real, &a.img);
    scanf("%f%f", &b.real, &b.img);
    
    c.real = a.real + b.real;
    c.img = a.img + b.img;
    
    printf("Sum = %.1f + %.1fi", c.real, c.img);
    
    return 0;
}
