#include<stdio.h>

int main()
{
    int n1,n2;
    
    printf("Enter First number: ");
    scanf("%d",&n1);
    printf("Enter Second number: ");
    scanf("%d",&n2);
    
    printf("Post Increment value is %d\n",n1--);
    printf("%d\n",n1);
    printf("Pre Increment value is %d\n",--n2);
    printf("%d\n",n2);
    
    return 0;
}
